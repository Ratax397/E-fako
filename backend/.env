# Configuration de base
APP_NAME=Waste Management API
APP_VERSION=1.0.0
DEBUG=true
LOG_LEVEL=INFO

# Configuration de la base de données MySQL
DATABASE_URL=mysql+pymysql://waste_user:waste_password@localhost:3306/waste_management_db
DATABASE_URL_ASYNC=mysql+aiomysql://waste_user:waste_password@localhost:3306/waste_management_db

# Configuration JWT
JWT_SECRET_KEY=your-jwt-secret-key-change-this-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=30

# Chiffrement biométrique
BIOMETRIC_ENCRYPTION_KEY=viPCmvxyL-IxwRpYQqMfgzO27zD2bI4YdKql0Z5jqmI=

# Redis (optionnel)
REDIS_URL=redis://localhost:6379/0

# Socket.IO
SOCKETIO_SECRET_KEY=your-socketio-secret-key-change-this-in-production

# Notifications push (optionnel)
FCM_SERVER_KEY=
FCM_SENDER_ID=

# Configuration CORS (origines autorisées)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:8080

# Celery (optionnel)
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# Reconnaissance faciale
FACE_RECOGNITION_TOLERANCE=0.6
FACE_RECOGNITION_MODEL=large